<%-include('./cabecalho.ejs')%>
<!-- page content -->
<div class="right_col" role="main">
  <div class="">
    <div class="page-title">
      <div class="title_left">
        <h3> Minhas Fotos </h3>
      </div>

      <div class="title_right">
        <div class="col-md-5 col-sm-5   form-group pull-right top_search">
          <div class="input-group">
            
            </span>
          </div>
        </div>
      </div>
    </div>

    <div class="clearfix"></div>

    <div class="row">
      <div class="col-md-12">
        <div class="x_panel">
          <div class="x_title">
            <h2> Minha Galeria </small></h2>

            <div class="clearfix"></div>
          </div>
          <div class="x_content">

            <div class="row">
              <%Fotos.forEach(function(foto){%>
              <!--INICIO IMAGEM-->
              <div class="col-md-55">
                <div class="thumbnail">
                  <div class="image view view-first">
                    <img style="width: 100%; display: block;" src="/fotos/<%=foto.foto%>" alt="image" />
                    <div class="mask">
                      <div class="tools tools-bottom">
                        <a href="#"><i class="fa fa-link"></i></a>
                        <a href="#"><i class="fa fa-pencil"></i></a>
                        <a href="#"><i class="fa fa-trash-o"></i></a>
                      </div>
                    </div>
                  </div>
                  <div class="caption">
                    <p><%=foto.descricao%></p>
                  </div>
                </div>
              </div>
              <% }) %>
              <!--FIM IMAGEM-->
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- /page content -->
<%-include('./rodape.ejs')%>